<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/mainHomeFragment">

    <fragment
        android:id="@+id/mainHomeFragment"
        android:name="com.example.hitepro_app.MainHomeFragment"
        android:label="main_fragment_home"
        tools:layout="@layout/main_fragment_home" />
    <fragment
        android:id="@+id/mainAutomationFragment"
        android:name="com.example.hitepro_app.MainAutomationFragment"
        android:label="main_fragment_automation"
        tools:layout="@layout/main_fragment_automation" >
        <argument android:name="comeBack" />
    </fragment>
    <fragment
        android:id="@+id/mainDevicesFragment"
        android:name="com.example.hitepro_app.MainDevicesFragment"
        android:label="main_fragment_devices"
        tools:layout="@layout/main_fragment_devices" />
    <fragment
        android:id="@+id/mainSettingsFragment"
        android:name="com.example.hitepro_app.MainSettingsFragment"
        android:label="main_fragment_settings"
        tools:layout="@layout/main_fragment_settings"/>
</navigation>